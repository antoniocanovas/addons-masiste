<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <record id="crm_lead_view_form_masiste" model="ir.ui.view">
        <field name="name">Masiste SALE ORDER INH view form</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <field name="partner_shipping_id" position="after">
                <field name="delegacion_id" attrs="{'domain':[('parent_id','=',record.partner_id.id),
                                                    ('company_type','!=','person')]}"/>
                <field name="contacto_id" attrs="{'domain':[('parent_id','=',record.partner_id.id),
                                                    ('company_type','=','person')]}"/>
            </field>
        </field>
    </record>

</odoo>
