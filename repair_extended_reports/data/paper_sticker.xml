<odoo>

    <data>
        <record id="paper-sticker" model="report.paperformat">
            <field name="name">Sticker-57x25</field>
            <field name="format">custom</field>
            <field name="page_height">57</field>
            <field name="page_width">25</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">2</field>
            <field name="margin_bottom">2</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_spacing">10</field>
            <field name="dpi">90</field>
            <field name="report_ids" eval="[(6, 0, [ref('repair_extended_reports.repairorder_extended_report_sticker')])]"/>
        </record>
    </data>


</odoo>
